<?xml version="1.0" encoding="gb2312" standalone="no"?>
<Configuration>
    <!-- 算法描述 -->
    <Algo name="QualityInspection" library="QualityInspection" paramCount="4" description="辐射校正质量评价">
        <param index="0" name="strIputFile" type="file" description="输入文件" typeParam='{"direction":"0", "filter":"Raster Files (*.tif *.tiff *.img);;All Files (*.*)"}'/>
        <param index="1" name="strOutputFile" type="file" direction="1" description="输出文件" typeParam='{"direction":"1", "filter":"txt Files (*.txt)"}'/>
		<param index="2" name="strCheckFile" type="file" direction="1" description="观测点文件" typeParam='{"direction":"1", "filter":"txt Files (*.txt)"}'/>
        <param index="3" name="satelliteID" type="int" direction="1" description="需要给定卫星类型，用于获取波长信息"/>
    </Algo>
</Configuration>